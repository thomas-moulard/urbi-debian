  var " + name + ".workEnded = 0;
  var " + name + ".startTime = time();
  // Setup transitions.
  var ret_transtion;
  ret_transitions = transitions () |
    {
      stop " + name + ".work;
      return ret_transitions
    },
  // Dispatch and work.
  " + name + ".start+begin+end:
    {
